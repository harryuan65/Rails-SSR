{
  "compilerOptions": {
    "module": "ESNext",
    "target": "es2020",
    "types": [
      "vite/client"
    ],
    "lib": [
      "ESNext",
      "DOM"
    ],
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./app/frontend/*"
      ],
      "~/*": [
        "./app/frontend/*"
      ]
    },
    "jsx": "react"
  },
  "exclude": [
    "**/dist",
    "**/node_modules",
    "**/test"
  ],
}